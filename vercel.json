{
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python@3.1.0"
    },
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
